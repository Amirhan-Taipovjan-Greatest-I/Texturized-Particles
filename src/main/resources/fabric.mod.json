{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${mod_description}",
    "authors": [
        "${mod_authors}"
    ],
    "contact": {
        "issues": "https://discord.com/invite/NZzxdkrV4s"
    },
    "license": "${mod_license}",
    "icon": "icon/icon.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "net.lopymine.tp.TexturizedParticles"
        ],
        "client": [
            "net.lopymine.tp.client.TexturizedParticlesClient"
        ],
        "modmenu": [
            "net.lopymine.tp.modmenu.ModMenuIntegration"
        ]
    },
    "depends": {
        "fabricloader": ">=${fabric_loader}",
        "minecraft": ">=${minecraft}",
        "java": ">=${java}",
        "${fabric_api_id}": "*"
    },
    "accessWidener": "aws/${minecraft}.accesswidener",
    "mixins": [
        "${mod_id}.mixins.json"
    ]
}